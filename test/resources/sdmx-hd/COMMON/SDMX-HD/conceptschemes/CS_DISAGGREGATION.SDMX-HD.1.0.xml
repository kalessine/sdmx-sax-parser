			<structure:Concept id="AGROUP"
				urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=SDMX-HD:CS_DISAGGREGATION[1.0].AGROUP">
				<structure:Name xml:lang="en">Age Group</structure:Name>
				<structure:Description xml:lang="en">A group of age ranges by which observational
					data may be disaggregated</structure:Description>
			</structure:Concept>
			<structure:Concept id="EDUC"
				urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=SDMX-HD:CS_DISAGGREGATION[1.0].EDUC">
				<structure:Name xml:lang="en">Education</structure:Name>
				<structure:Description xml:lang="en">Education level of the population for which
					data are collected.</structure:Description>
			</structure:Concept>
			<structure:Concept id="GENDER"
				urn="urn:sdmx:org.sdmx.infomodel.conceptscheme.Concept=SDMX-HD:CS_DISAGGREGATION[1.0].GENDER">
